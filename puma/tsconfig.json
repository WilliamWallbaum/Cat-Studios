{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@blog/*": ["src/pages/blog/*"],
      "@pages/*": ["src/pages/*"],
      "@layouts/*": ["src/layouts/*"],
      "@sections/*": ["src/layouts/sections/*"],
      "@page/*": ["src/layouts/page/*"],
      "@styles/*": ["src/styles/*"]
    },
  }
}